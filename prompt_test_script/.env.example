# Example Environment Variables for AI Detector Evaluation
# Copy this file to .env and fill in your actual values

# ============================================================================
# API Keys (Required for text generation)
# ============================================================================

# OpenAI API Key for GPT models
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key for Claude models
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ============================================================================
# Detector API Configuration (Required for testing)
# ============================================================================

# Base URL of your AI detector API
DETECTOR_API_URL=http://localhost:8000

# Authentication token for detector API (if required)
DETECTOR_API_TOKEN=your_detector_token_here

# ============================================================================
# OpenTelemetry Metrics (Required for production monitoring)
# ============================================================================

# Prometheus Remote Write endpoint (Mimir, Cortex, etc.)
PROMETHEUS_REMOTE_WRITE_URL=http://localhost:9009/api/v1/push

# Mimir tenant ID (required for multi-tenant setups)
PROMETHEUS_TENANT_ID=anonymous

# ============================================================================
# Optional Metadata (For production tracking and filtering)
# ============================================================================

# Unique identifier for this evaluation run
RUN_ID=eval-local

# Dataset version being tested
DATASET_VERSION=v1.0

# Git commit SHA for reproducibility
GIT_COMMIT_SHA=

# Environment name (dev, staging, production)
ENVIRONMENT=local

# Cluster or region identifier
CLUSTER=local-test


# Git commit SHA for reproducibility
GIT_COMMIT_SHA=