# Example Environment Variables for AI Detector Evaluation
# Copy this file to .env and fill in your actual values

# ============================================================================
# API Keys (Required for text generation)
# ============================================================================

# OpenAI API Key for GPT models
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key for Claude models
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ============================================================================
# Detector API Configuration (Required for testing)
# ============================================================================

# Base URL of your AI detector API
DETECTOR_API_URL=http://localhost:8000

# Authentication token for detector API (if required)
DETECTOR_API_TOKEN=your_detector_token_here

# ============================================================================
# Prometheus Metrics (Optional, for production monitoring)
# ============================================================================

# Prometheus Pushgateway URL (leave empty to disable)
PUSHGATEWAY_URL=

# Prometheus Remote Write URL (alternative to Pushgateway for serverless)
PROMETHEUS_REMOTE_WRITE_URL=

# Prometheus Remote Write Authentication (if required)
PROMETHEUS_REMOTE_WRITE_USER=
PROMETHEUS_REMOTE_WRITE_PASSWORD=

# ============================================================================
# S3 Storage (Optional, for serverless/cloud deployments)
# ============================================================================

# S3 bucket name for storing results
S3_BUCKET_NAME=

# AWS credentials (or use IAM roles/instance profiles)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_DEFAULT_REGION=us-east-1

# ============================================================================
# Run Metadata (Optional, useful for tracking in production)
# ============================================================================

# Unique identifier for this evaluation run
RUN_ID=

# Dataset version being tested
DATASET_VERSION=

# Git commit SHA for reproducibility
GIT_COMMIT_SHA=